      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>K_i</ci>
            </apply>
            <apply>
               <times/>
               <apply>
                  <divide/>
                  <apply>
                     <times/>
                     <apply>
                        <minus/>
                        <cn cellml:units="mA_pA">1</cn>
                     </apply>
                     <apply>
                        <minus/>
                        <apply>
                           <plus/>
                           <ci>i_K1</ci>
                           <ci>i_to</ci>
                           <ci>i_Kr</ci>
                           <ci>i_Ks</ci>
                           <ci>i_p_K</ci>
                           <ci>i_Stim</ci>
                        </apply>
                        <apply>
                           <times/>
                           <cn cellml:units="dimensionless">2</cn>
                           <ci>i_NaK</ci>
                        </apply>
                     </apply>
                  </apply>
                  <apply>
                     <times/>
                     <cn cellml:units="litre_micrometre3">1</cn>
                     <ci>V_c</ci>
